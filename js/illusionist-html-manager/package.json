{
  "name": "@danielfrg/illusionist-html-manager",
  "version": "0.1.0",
  "description": "Package for rendering Jupyter widgets outside notebooks with offline capabilities",
  "homepage": "https://github.com/danielfrg/illusionist",
  "bugs": {
    "url": "https://github.com/danielfrg/illusionist/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielfrg/illusionist.git"
  },
  "license": "Apache-2.0",
  "author": "Daniel Rodriguez",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "dist/",
    "css/*.css"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:src && webpack && npm run build:embed-amd",
    "build:src": "tsc --build",
    "build:embed-amd": "node scripts/concat-amd-build.js",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo && rimraf dist",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^4.0.0-alpha.0",
    "@jupyter-widgets/base-manager": "^1.0.0-alpha.0",
    "@jupyter-widgets/controls": "^3.0.0-alpha.0",
    "@jupyter-widgets/output": "^4.0.0-alpha.0",
    "@jupyter-widgets/schema": "^0.5.0-alpha.0",
    "@jupyter-widgets/html-manager": "^0.20.0-alpha.0",
    "@jupyterlab/outputarea": "^2.0.2",
    "@jupyterlab/rendermime": "^2.0.2",
    "@jupyterlab/rendermime-interfaces": "^2.0.1",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@lumino/widgets": "^1.11.1",
    "jquery": "^3.1.1",
    "ajv": "^6.10.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.0",
    "chai": "^4.0.0",
