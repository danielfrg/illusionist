{
    "name": "@danielfrg/illusionist",
    "version": "0.1.2",
    "description": "Client-only Jupyter Widgets",
    "author": "Daniel Rodriguez",
    "license": "Apache-2.0",
    "homepage": "https://github.com/danielfrg/illusionist",
    "bugs": {
        "url": "https://github.com/danielfrg/illusionist/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/danielfrg/illusionist"
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "main": "src/index.js",
    "scripts": {
        "dev": "webpack --watch --mode=development",
        "dev:prod": "webpack --watch --mode=production",
        "build": "webpack --mode=production",
        "build:watch": "webpack --mode=production --watch",
        "fmt": "prettier --write \"src/**/*.{js,jsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
        "clean": "rm -rf .cache/ dist/ lib/",
        "reset": "npm run clean && rm -rf node_modules",
        "test": "jest --silent",
        "test:coverage": "jest --coverage",
        "test:update": "jest -u",
        "test:watch": "jest --watch",
        "publish": "npm version && npm publish --access public"
    },
    "dependencies": {
        "@jupyter-widgets/base": "4.0.0-alpha.1",
        "@jupyter-widgets/controls": "3.0.0-alpha.1",
        "@jupyter-widgets/html-manager": "0.20.0-alpha.1",
        "@jupyter-widgets/output": "4.0.0-alpha.1",
        "papaparse": "^5.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/preset-env": "^7.10.2",
        "@types/papaparse": "^5.0.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^3.5.3",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "file-loader": "^6.0.0",
        "jest": "^26.0.1",
        "mini-css-extract-plugin": "^0.9.0",
        "prettier": "^2.0.5",
        "sass": "^1.26.8",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    }
}
